<h1>Random Calculation</h1>

<form action="/random/results">
    <div>
      <label for="minimum">
          Minumum
      </label>
      <input id="minimum" type="text" name="minimum" placeholder="E.g. 1.5">
    </div>
    <div>
      <label for="maximum">
          Maximum
      </label>
      <input id="maximum" type="text" name="maximum" placeholder="E.g. 4.5">
    </div>
    <button>Pick random number</button>

</form>